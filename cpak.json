{
  "name": "Lutris",
  "description": "Lutris is a video game preservation platform aiming to keep your video game collection up and running for the years to come.",
  "version": "0.5.18",
  "image": "ghcr.io/containerpak/lutris:main",
  "binaries": [
    "/usr/bin/lutris"
  ],
  "desktop_entries": [
    "/usr/share/applications/lutris.desktop"
  ],
  "dependencies": [],
  "addons": [],
  "idle_time": 0,
  "override": {
    "socketX11": true,
    "socketWayland": true,
    "socketPulseAudio": true,
    "socketSessionBus": true,
    "socketSystemBus": false,
    "socketSshAgent": false,
    "socketCups": false,
    "socketGpgAgent": false,
    "socketAtSpiBus": false,
    "socketBluetooth": false,
    "deviceDri": false,
    "deviceKvm": false,
    "deviceShm": false,
    "deviceAlsa": false,
    "deviceVideo": false,
    "deviceFuse": false,
    "deviceTun": false,
    "deviceUsb": false,
    "deviceAll": true,
    "notification": true,
    "fsHost": false,
    "fsHostEtc": false,
    "fsHostHome": true,
    "fsExtra": [],
    "env": [],
    "network": true,
    "process": false,
    "asRoot": false,
    "allowedHostCommands": []
  }
}
